{
  "name": "express-vue-demo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "babel-server": "babel --config-file ./babel.config.node.js server --out-dir lib",
    "server": "npm run babel-server && npm run start",
    "dev": "nodemon --watch ./server/* --exec npm run server"
